{
  "version": 2,
  "buildCommand": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
  "builds": [
    {
      "src": "api/vercel_app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "handle": "filesystem"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel_app.py"
    }
  ]
}